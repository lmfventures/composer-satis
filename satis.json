{
    "name": "lmf/composer-satis",
    "homepage": "https://lmfventures.github.io/composer-satis",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lmfventures/package-ocr.git",
            "name": "lmf/package-ocr"
        },
        {
            "type": "vcs",
            "url": "git@github.com:lmfventures/package-custom-generator.git",
            "name": "lmf/custom-generator"
        }
    ],
    "require": {
        "lmf/package-ocr": "*",
        "lmf/custom-generator": "*"
    },
    "require-all": false,
    "require-dependencies": true,
    "require-dev-dependencies": false,
    "config": {
        "secure-http": true
    },
    "output-dir": "public",
    "output-html": true,
    "allow-seo-indexing": true,
    "archive": {
        "directory": "dist",
        "format": "tar",
        "prefix-url": "https://lmfventures.github.io/composer-satis",
        "skip-dev": true
    }
}
