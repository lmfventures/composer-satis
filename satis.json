{
    "name": "lmf/composer-satis",
    "homepage": "https://lmfventures.github.io/composer-satis",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lmfventures/package-ocr.git",
            "name": "lmf/package-ocr"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lmfventures/package-custom-generator.git",
            "name": "lmf/custom-generator"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lmfventures/sms-service.git",
            "name": "lmf/sms-service"
        }
    ],
    "require": {
        "lmf/package-ocr": "*",
        "lmf/custom-generator": "*",
        "lmf/sms-service": "*"
    },
    "require-all": false,
    "require-dependencies": false,
    "require-dev-dependencies": false,
    "config": {
        "secure-http": true
    },
    "output-dir": "public",
    "output-html": true,
    "allow-seo-indexing": true
}
